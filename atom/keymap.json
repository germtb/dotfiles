{
	"atom-workspace": {
		"ctrl-w": "core:close",
		"ctrl-1": "pane:show-item-1",
		"ctrl-2": "pane:show-item-2",
		"ctrl-3": "pane:show-item-3",
		"ctrl-4": "pane:show-item-4",
		"ctrl-5": "pane:show-item-5",
		"ctrl-6": "pane:show-item-6",
		"ctrl-7": "pane:show-item-7",
		"ctrl-8": "pane:show-item-8",
		"ctrl-9": "pane:show-item-9",
		"ctrl-t": "application:new-file",
		"ctrl-h": "pane:show-previous-item",
		"ctrl-l": "pane:show-next-item",
		"ctrl-shift-w": "pane:close",

		"cmd-t": "application:new-file",
		"cmd-shift-w": "pane:close"
	},
	"atom-text-editor": {
		"alt-cmd-enter": "hyperclick:confirm-cursor",
		"ctrl-d": "find-and-replace:select-next",
		"ctrl-f": "find-and-replace:show"
	},
	"atom-text-editor.vim-mode-plus": {
		"ctrl-t": "application:new-file"
	},
	"atom-text-editor.vim-mode-plus.insert-mode": {
		"j j": "vim-mode-plus:activate-normal-mode"
	},
	"atom-text-editor.vim-mode-plus.normal-mode": {
		"O": "editor:newline-above",
		"o": "editor:newline-below"
	}
}
