#!/bin/bash
mv -f ~/.vimrc ~/.old_vimrc
ln -s ~/dotfiles/vim/.vimrc ~/.vimrc

mv -f ~/.zshrc ~/.old_zshrc
ln -s ~/dotfiles/shell/.bashrc ~/.zshrc

mv -f ~/.bashrc ~/.old_bashrc
ln -s ~/dotfiles/shell/.bashrc ~/.bashrc

mv -f ~/.bash_profile ~/.old_bash_profile
ln -s ~/dotfiles/shell/.bashrc ~/.bash_profile

mv -f ~/.babunrc ~/.old_babunrc
ln -s ~/dotfiles/shell/.bashrc ~/.babunrc

